<div class="home">
  <div class="home__wrapper">
    <h1><%= home.data.label %></h1>


    <div class="home__gallery">
      <% home.data.gallery.forEach(media => { %>
       <figure class="home__gallery__media">
         <img src="<%= media.image.url %>" alt="<%= media.image.alt %>">
       </figure>
      <% }) %>
    </div>
    <a class="home__button" href="<%= ctx.linkHandler(home.data.collections) %>"><%= home.data.button %></a>
  </div>
</div>
